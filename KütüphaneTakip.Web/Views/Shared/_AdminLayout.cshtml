                        <li class="nav-item">
                            <a href="~/Admin/Index" class="nav-link ">
                                <i class="nav-icon fas fa-user"></i>
                                <p>
                                    Kullanıcı Tanımlama
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Admin/Department" class="nav-link">
                                <i class="nav-icon far fa-building"></i>
                                <p>
                                    Departman Tanımlama
                                </p>
                            </a>
