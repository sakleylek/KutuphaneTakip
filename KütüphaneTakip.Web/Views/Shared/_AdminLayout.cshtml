                </li>
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a id="Information" class="nav-link">User</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="~/Admin/Quit" role="button">  Çıkış Yap</a>
                </li>
            </ul>
        </nav>
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a class="brand-link d-flex justify-content-center">
                <span class="brand-text font-weight-light">İş Takip</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
